[ -n "$RATOS_CONFIG_SHIP" ] || RATOS_CONFIG_SHIP=https://github.com/Artemis-CNC/RatOS-configuration.git
[ -n "$RATOS_CONFIG_BRANCH" ] || RATOS_CONFIG_BRANCH=hevo-v2.x
[ -n "$RATOS_THEME_SHIP" ] || RATOS_THEME_SHIP=https://github.com/Artemis-CNC/RatOS-theme.git
[ -n "$RATOS_THEME_BRANCH" ] || RATOS_THEME_BRANCH=hevo-v2.x
[ -n "$RATOS_CONFIGURATOR_SHIP" ] || RATOS_CONFIGURATOR_SHIP=https://github.com/Artemis-CNC/RatOS-configurator.git
[ -n "$RATOS_CONFIGURATOR_BRANCH" ] || RATOS_CONFIGURATOR_BRANCH=v2.x-deployment
